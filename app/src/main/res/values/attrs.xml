<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!-- format:该属性的取值类型
     一共有：string,color,demension,integer,enum,
     reference,float,boolean,fraction,flag

     -->
    <attr name="titleText" format="string"/>  <!-- 字体-->
    <attr name="titleTextColor" format="color"/>  <!--字体颜色 -->
    <attr name="titleTextSize" format="dimension"/>  <!--字体大小 -->
    <declare-styleable name="CustomTextView">
        <attr name="titleText"/>
        <attr name="titleTextColor" />
        <attr name="titleTextSize" />
    </declare-styleable>



</resources>